<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                arm_port:
                    port_name: "/dev/ttyUSB0"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 10
                    update_rate: 20
        </rosparam>
    </node>

    <rosparam file="$(find ric_robot)/config/komodo_arm_elev.yaml" command="load"/>
    <node name="arm_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port=arm_port
                base_rotation_controller
                shoulder_controller
		elbow1_controller
		elbow2_controller
		wrist_controller
		left_finger_controller
		right_finger_controller
                elevator_controller"
          output="screen"/>

<node name="dynamixel_joint_state_publisher" pkg="ric_robot" type="dynamixel_joint_state_publisher.py" />

</launch>
